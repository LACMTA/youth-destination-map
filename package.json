  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.37",
    "airtable": "^0.12.2",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "papaparse": "^5.4.1"
  }
}
